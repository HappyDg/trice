[Plugin]
Name=per_stm32f4xx_i2c
Implement=per_stm32_i2c
Caption=STM32F4XX I2C
Description=STMicroelectronics STM32F4XX I2C
Require0=stm32f4xx_lib
Require1=stm32_pinmapper
HardwareId=STM32F4XX_I2C
Instance0=STM32F4XX_I2C1
Instance1=STM32F4XX_I2C2
Instance2=STM32F4XX_I2C3

[Available_If0]
Kind=ENVIRONMENT
Object=DEVICE
Condition=REQ
Value=STM32F4.

[Instance]
Require0=

[Instance_Option0]
Name=baseaddress
Instance0_DefaultValue=0x40005400
Instance1_DefaultValue=0x40005800
Instance2_DefaultValue=0x40005C00
Caption=Base Address
Kind=UINT32
DefaultValue=0xFFFFFFFF
Auto=True

[Instance_Option1]
Name=pinconfig
Caption=Pin Configuration
Description=This function is generated by the Pin Mapper and sets up the pins for the peripheral.
Kind=CHANDLER
Instance0_DefaultValue=pincfg_i2c1_init
Instance1_DefaultValue=pincfg_i2c2_init
Instance2_DefaultValue=pincfg_i2c3_init
Return=void
Param=void
Auto=True

[Instance_Option2]
Name=Interruptev
Instance0_DefaultValue=31
Instance1_DefaultValue=33
Instance2_DefaultValue=72
Caption=Interruptev
Description=Event interrupt
Kind=INTERRUPT
Auto=True

[Instance_Option3]
Name=Interrupter
Instance0_DefaultValue=32
Instance1_DefaultValue=34
Instance2_DefaultValue=73
Caption=Interrupter
Description=Error interrupt
Kind=INTERRUPT
Auto=True

