[Plugin]
Name=per_stm32f30x_i2c
Implement=per_stm32_i2c
Caption=STM32F30X I2C
Description=STMicroelectronics STM32F30X I2C
Require0=stm32f30x_lib
Require1=stm32_pinmapper
HardwareId=STM32F30X_I2C
Instance0=STM32F30X_I2C1
Instance1=STM32F30X_I2C2

[Available_If0]
Kind=ENVIRONMENT
Object=DEVICE
Condition=REQ
Value=STM32F3[01].
OrCondition=True

[Available_If1]
Kind=ENVIRONMENT
Object=DEVICE
Condition=REQ
Value=STM32F3[1259]8.
OrCondition=True

[Available_If2]
Kind=ENVIRONMENT
Object=DEVICE
Condition=REQ
Value=STM32F334.
OrCondition=True

[Instance]
Require0=

[Instance_Option0]
Name=baseaddress
Instance0_DefaultValue=0x40005400
Instance1_DefaultValue=0x40005800
Caption=Base Address
Kind=UINT32
DefaultValue=0xFFFFFFFF
Auto=True

[Instance_Option1]
Name=pinconfig
Caption=Pin Configuration
Description=This function is generated by the Pin Mapper and sets up the pins for the peripheral.
Kind=CHANDLER
Instance0_DefaultValue=pincfg_i2c1_init
Instance1_DefaultValue=pincfg_i2c2_init
Return=void
Param=void
Auto=True

[Instance_Option2]
Name=interrupt0
Instance0_DefaultValue=31
Instance1_DefaultValue=33
Caption=interrupt0
Description=Event interrupt
Kind=INTERRUPT
Auto=True

[Instance_Option3]
Name=interrupt1
Instance0_DefaultValue=32
Instance1_DefaultValue=34
Caption=interrupt1
Description=Error interrupt
Kind=INTERRUPT
Auto=True

