[Plugin]
Name=per_stm32f0xx_i2c
Implement=per_stm32_i2c
Caption=STM32F0XX I2C
Description=STMicroelectronics STM32F0XX I2C
Require0=stm32f0xx_lib
Require1=stm32_pinmapper
HardwareId=STM32F0XX_I2C
Instance0=STM32F0XX_I2C1
Instance1=STM32F0XX_I2C2

[Available_If0]
Kind=ENVIRONMENT
Object=DEVICE
Condition=REQ
Value=STM32F0.

[Instance]
Require0=

[Instance_Option0]
Name=baseaddress
Instance0_DefaultValue=0x40005400
Instance1_DefaultValue=0x40005800
Caption=Base Address
Kind=UINT32
DefaultValue=0xFFFFFFFF
Auto=True

[Instance_Option1]
Name=pinconfig
Caption=Pin Configuration
Description=This function is generated by the Pin Mapper and sets up the pins for the peripheral.
Kind=CHANDLER
Instance0_DefaultValue=pincfg_i2c1_init
Instance1_DefaultValue=pincfg_i2c2_init
Return=void
Param=void
Auto=True

[Instance_Option2]
Name=interrupt
Instance0_DefaultValue=23
Instance1_DefaultValue=24
Caption=interrupt
Description=Global interrupt
Kind=INTERRUPT
Auto=True

